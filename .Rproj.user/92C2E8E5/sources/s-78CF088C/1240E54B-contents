library(tidyverse)
library(lubridate)

read_csv('../../../Downloads/Rewards.csv') %>% View()

test <- read_csv('../../../Downloads/Rewards.csv') %>%
  mutate(time = as_datetime(time)) %>%
  mutate(reward_percent = reward/supply) %>%
  summarise(
    total_reward_percent = (sum(reward_percent)/nrow(.)) * 365 * 100
  )


read_csv('../../../Downloads/Rewards.csv') %>%
  select(time) %>% pull() %>% unique() %>% as_datetime() %>% as.Date() %>% unique()

